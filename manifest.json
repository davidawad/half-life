{
   "background": {
      "persistent": false,
      "scripts": [ "motivation.js" ]
   },
   "chrome_url_overrides": {
      "newtab": "dashboard.html"
   },
   "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
   "description": "Replace new tab page with Motivation",
   "icons": {
      "16": "icon-16.png",
      "32": "icon-16.png",
      "192": "icon-128.png",
      "384": "icon-384.png"
   },
   "manifest_version": 2,
   "name": "Half Life",
   "offline_enabled": true,
   "version": "0.6"
}
